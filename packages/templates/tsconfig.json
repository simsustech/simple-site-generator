{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["vue", "vite/client", "node"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "resolveJsonModule": true,
    "target": "ESNext",
    "outDir": "./dist",
    "declaration": true,
    "declarationDir": "dist/types"
  }
}
